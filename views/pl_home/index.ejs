    //Created / Generates the captcha function
    function DrawCaptchaForPoll() {
        var a = Math.ceil(Math.random() * 10) + '';
        var b = Math.ceil(Math.random() * 10) + '';
        var c = Math.ceil(Math.random() * 10) + '';
        var d = Math.ceil(Math.random() * 10) + '';
        var e = Math.ceil(Math.random() * 10) + '';
        var f = Math.ceil(Math.random() * 10) + '';
        var g = Math.ceil(Math.random() * 10) + '';
        var code = a + ' ' + b + ' ' + c + ' ' + d + ' ' + e + ' ' + f + ' ' + g;
        if ($("#txtCaptchaForPoll").length > 0) {
            document.getElementById("txtCaptchaForPoll").value = code
        }
    }

    // Validate the Entered input aganist the generated security code function
    function ValidatePoll() {
        var select = $("input[type=hidden][id$='hdfSelected']").val();
        if (select == '') {
            alert("Bạn chưa chọn đáp án");
            return false;
        }
        var str1 = removeSpaces(document.getElementById('txtCaptchaForPoll').value);
        var str2 = removeSpaces(document.getElementById('txtMaxacnhanForPoll').value);
        if (str1 == str2) {
            $("input[type=hidden][id$='hdfCaptcha']").val('true');
            return true;
        } else {
            $("input[type=hidden][id$='hdfCaptcha']").val('false');
            alert("Bạn nhập sai mã xác nhận");
            return false;
        }

    }

    // Remove the spaces from the entered and generated code
    function removeSpaces(string) {
        return string.split(' ').join('');
    }

    $("#subscriptions input") // select the radio by its id
        .on('change', function () { // bind a function to the change event
            var value = $('input[name="poll"]:checked', '#subscriptions').val();
            $("input[type=hidden][id$='hdfSelected']").val(value);
        });
</script>

<script type="text/javascript">
    $(document).ready(function () {

        var data = $("#hdThongTinCong").val();
        if (data != undefined && data != "") {
            var objd = $.parseJSON(data);
            //process first line

            var tabroot = "<div class=\"tab-content border tab-c-ctps\">";
            var tabhtml = "<ul class=\"nav nav- tabs tab-subpt tab-pa-xs tabcs\" role=\"tablist\">";
            $.each(objd, function (j, k) {

                var tappanel = "";
                if (j === 0) {
                    $.each(k.Values, function (i, n) {
                        tabhtml += "<li role=\"presentation\" class=\"\"> <a href=" + n.Value + " target=\"_self\"> <i class=\"fa fa-caret-right\" aria-hidden=\"true\"></i> " + n.Name + "</a></li>";
                    });
                } else {

                    if (j === (objd.length - 1)) {
                        tabhtml += "<li role=\"presentation\" class=\"active\"><a href=" + '#CTP' + j + " aria-controls=\"settings\" role=\"tab\" data-toggle=\"tab\">" + k.Name + "</a></li>";
                        tappanel += "<div role=\"tabpanel\" class=\"tab-pane active \" id=" + 'CTP' + j + ">";
                    } else {
                        tabhtml += "<li role=\"presentation\" class=\"\"><a href=" + '#CTP' + j + " aria-controls=\"settings\" role=\"tab\" data-toggle=\"tab\">" + k.Name + "</a></li>";
                        tappanel += "<div role=\"tabpanel\" class=\"tab-pane \" id=" + 'CTP' + j + ">";
                    }
                    var tappanel1 = "<div class=\"section-content\"><div class=\"row\"><div class=\"col- md - 12\">";
                    tappanel1 += "<ul class=\"bullet-rectangle no-content\">";

                    $.each(k.Values, function (a, b) {
                        tappanel1 += "<li class=\"col-md-6\"><a href=" + b.Value + " target=\"_self\"> <i class=\"fa fa-caret-right\" aria-hidden=\"true\"></i> " + b.Name + "</a></li> ";
                    });
                    tappanel1 += "</ul>";
                    tappanel += tappanel1 + "</div></div><div class=\"clearfix\"></div></div></div>";
                }
                tabroot += tappanel;
            });
            tabroot += "</div>";
            tabhtml += "</ul>";
            var html = tabhtml + tabroot;
            $("#congtp-div").html(html);
        }
    });
</script>

